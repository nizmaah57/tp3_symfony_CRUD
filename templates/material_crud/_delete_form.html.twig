<form method="post" action="{{ path('app_material_crud_delete', {'id': material.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet item ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ material.id) }}">
    <button class="btn btn-danger">Supprimer la matière</button>
</form>
